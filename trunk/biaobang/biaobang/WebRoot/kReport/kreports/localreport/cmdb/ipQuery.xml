<?xml version="1.0" encoding="GB2312"?> 
<kreport>
	<name>ipQuery</name>
	<display-name>IP查询</display-name>
	<pageable>true</pageable>
	<condition-size>4</condition-size>
	<skin></skin>	
	<actions>
		<action name="query">
			<index>0</index>
			<display-name>查询</display-name>
			<do>doQuery()</do>
		</action>
		<action name="reset">
			<index>2</index>
			<display-name>重置</display-name>
			<do>doReset()</do>
		</action>
		<action name="export">
			<index>1</index>
			<display-name>导出</display-name>
			<do>doExport()</do>
		</action>
	</actions>	
	<conditions>	
		<condition name="ip">
			<index>10</index>
			<display-name>IP</display-name>
			<type>text</type>
			<return-type>void</return-type>
		</condition>
		<condition name="hostname">
			<index>20</index>
			<display-name>机器名</display-name>
			<type>text</type>
			<return-type>like</return-type>
		</condition>
		<condition name="startDate">
			<index>30</index>
			<display-name>启用时间(开始)</display-name>
			<type>date</type>
			<return-type>date</return-type>
			<extend>yyyy-MM-dd HH:mm</extend>
			<default-value></default-value>
		</condition>
		<condition name="endDate">
			<index>40</index>
			<display-name>启用时间(结束)</display-name>
			<type>date</type>
			<return-type>date</return-type>
			<extend>yyyy-MM-dd HH:mm</extend>
			<default-value></default-value>
		</condition>
		<condition name="purpose">
			<index>50</index>
			<display-name>用途</display-name>
			<type>text</type>
			<return-type>like</return-type>
		</condition>
		<condition name="username">
			<index>60</index>
			<display-name>使用人</display-name>
			<type>text</type>
			<return-type>like</return-type>
		</condition>
		<condition name="remark">
			<index>70</index>
			<display-name>备注</display-name>
			<type>text</type>
			<return-type>like</return-type>
		</condition>
	</conditions>	
	<ksql><![CDATA[
		select t.hostname 机器名,
		       t.ip IP,
		       t.purpose 用途,
		       t.username 使用人,
		       t.usedate 启用时间,
		       t.remark 备注
		from qry_ip t
		where 1=1
		  #[and t.ip like '${ip}%']
		  #[and t.hostname like ${hostname}]
		  #[and t.purpose like ${purpose}]
		  #[and t.username like ${username}]
		  #[and t.usedate>=${startDate}]
		  #[and t.usedate<${endDate}]
		  #[and t.remark like ${remark}]
		order by regexp_substr(t.ip,'\d{1,3}',1,1)+0,regexp_substr(t.ip,'\d{1,3}',1,2)+0,regexp_substr(t.ip,'\d{1,3}',1,3)+0,regexp_substr(t.ip,'\d{1,3}',1,4)+0

		]]></ksql>
</kreport>