<?xml version="1.0" encoding="GB2312"?> 
<kreport>
	<name>satisfactionForUser</name>
	<display-name>用户满意度统计</display-name>
	<page-size></page-size>
	<condition-size>4</condition-size>
	<skin></skin>	
	<actions>
		<action name="query">
			<index>10</index>
			<display-name>查询</display-name>
			<do>doQuery()</do>
		</action>
		<action name="export">
			<index>20</index>
			<display-name>导出</display-name>
			<do>doExport()</do>
		</action>
		<action name="exportOthers">
			<index>30</index>
			<display-name>导出清单</display-name>
			<do>doExportOther('/itsm/localreport/satisfactionForUserBill')</do>
		</action>
		<action name="reset">
			<index>100</index>
			<display-name>重置</display-name>
			<do>doReset()</do>
		</action>
	</actions>	
	<conditions>
		<condition name="cStartDate">
			<index>10</index>
			<display-name>归档时间(开始)</display-name>
			<type>date</type>
			<return-type>date</return-type>
			<extend>yyyy-MM-dd HH:mm</extend>
			<default-value>-1M.0H.0m.0s</default-value>
		</condition>
		<condition name="cEndDate">
			<index>20</index>
			<display-name>归档时间(结束)</display-name>
			<type>date</type>
			<return-type>date</return-type>
			<extend>yyyy-MM-dd HH:mm</extend>
			<default-value>.0H.0m.0s</default-value>
		</condition>
		<condition name="rStartDate">
			<index>30</index>
			<display-name>回单时间(开始)</display-name>
			<type>date</type>
			<return-type>date</return-type>
			<extend>yyyy-MM-dd HH:mm</extend>
			<default-value></default-value>
		</condition>
		<condition name="rEndDate">
			<index>40</index>
			<display-name>回单时间(结束)</display-name>
			<type>date</type>
			<return-type>date</return-type>
			<extend>yyyy-MM-dd HH:mm</extend>
			<default-value></default-value>
		</condition>	
		<condition name="area">
			<index>50</index>
			<display-name>区域</display-name>
			<type>select</type>
			<return-type>string</return-type>
			<extend><![CDATA[
				select d.dvalue,d.dname from sys_data_dictionary d where d.dgroup='区域' order by d.ordernum
			]]></extend>
		</condition>	
		<condition name="isflowITZX">
			<index>60</index>
			<display-name>是否流经IT中心</display-name>
			<type>select</type>
			<extend>{exists:是,not exists:否}</extend>
		</condition>	
	</conditions>	
	<data>
		<tbody><![CDATA[
			<tr style='font-size:9pt;font-family:宋体;line-height:14pt;	background-color:white;'>
				<td align='center'><a href="javascript:openList('/itsm/localreport/satisfactionForUserBill','${1}','satisfaction=GZ_SAT_001')">${1}</a></td>
				<td align='center'><a href="javascript:openList('/itsm/localreport/satisfactionForUserBill','${2}','satisfaction=GZ_SAT_002')">${2}</a></td>
				<td align='center'><a href="javascript:openList('/itsm/localreport/satisfactionForUserBill','${3}','satisfaction=GZ_SAT_003')">${3}</a></td>
				<td align='center'><a href="javascript:openList('/itsm/localreport/satisfactionForUserBill','${4}','satisfaction=GZ_SAT_004')">${4}</a></td>
				<td align='center'><a href="javascript:openList('/itsm/localreport/satisfactionForUserBill','${4}','satisfaction=GZ_SAT_005')">${5}</a></td>
			</tr>]]>
		</tbody>
	</data>	
	<ksql><![CDATA[
		--用户满意度统计
		select nvl(sum(decode(i.satisfaction,'GZ_SAT_001',1,0)),0) 非常满意,
		       nvl(sum(decode(i.satisfaction,'GZ_SAT_002',1,0)),0) 满意,
		       nvl(sum(decode(i.satisfaction,'GZ_SAT_003',1,0)),0) 一般,
		       nvl(sum(decode(i.satisfaction,'GZ_SAT_004',1,0)),0) 不满意,
		       nvl(sum(decode(i.satisfaction,'GZ_SAT_005',1,0)),0) 差
		from rec_flow f,ins_frm_fault i
		where f.flowlsh=i.flowlsh
		  and f.status!='XJ'
		  and i.satisfaction is not null
		  #[and i.area=${area}]
		  #[and ${isflowITZX} (select 1 from rec_node n where n.flowlsh=f.flowlsh and n.nodecode in ('8704d4d70a11226800834bbe3cb68d92','45c80d11c0a86b4e01ea1f505eae726f'))]
		  #[and i.replytime>=${rStartDate}]
		  #[and i.replytime<${rEndDate}]
		  #[and f.completedate>=${cStartDate}]
		  #[and f.completedate<${cEndDate}]
		]]></ksql>
</kreport>